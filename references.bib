@techreport{bolt_harald_bernd_lohr_wolfgang_meisner_albrecht_einschatzung_2013,
	title = {Einschätzung der technischen {Risiken} bei {ITER}},
	abstract = {Anlass und Auftrag Fusionsforschung an ITER ist dem ‚Europäischen Strategieplan für Energietechnologie' (SET-Plan) zufolge ein Element der europäischen Energiestrategie. Der SET-Plan definiert als langfristiges Ziel, eine kohlenstoffneutrale Wirtschaft und nachhaltige Energieversorgung zu verwirklichen und hierzu mit bahnbrechenden Neuerungen eine neue Technologiegeneration zu entwickeln. Europa spielt in der Fusionsforschung und der zugehörigen Technologie weltweit eine Vorreiterrolle. Durch den Bau und Betrieb von ITER wird eine solide wissenschaftlich-technologische Basis für die Entscheidung über weitere Schritte in Richtung von Fusion als einer reellen Energiequelle geschaffen werden. ITER ist von daher für die Bundesregierung ein wichtiges Projekt, das aufmerksamer Begleitung bedarf. ITER ist wegen großer Kostensteigerungen, Zeitverzögerungen und mangelhaftem Management in die Kritik geraten. Aus Sicht des Bundesministeriums für Bildung und Forschung (BMBF) wird ein Erfolg von ITER vor allem von der Bewältigung noch vorhandener Risiken bei einigen komplexen Komponenten abhängen. Eine Gruppe unabhängiger Experten (siehe Anhang) wurde deshalb mit Schreiben des BMBF vom 12.02.2013 beauftragt, den derzeitigen Stand des Projektes zu begutachten. Hierbei sollten erstens die größten technischen Risiken bei den verschiedenen Komponenten und die Möglichkeiten, diese Risiken zu verringern, analysiert werden. Zweitens sollte der ursprüngliche und der vom F4E(Fusion for Energy)-Management aufgestellte Zeitplan verglichen, die Risiken für weitere Zeitverzögerungen untersucht und eine Abschätzung des Zeitpunkts einer Inbetriebnahme gemacht werden. Drittens wurden die Experten gebeten mögliche Implikationen mit Hinblick auf die Kosten aufzuzeigen. Viertens sollten Maßnahmen im Management benannt werden, die es ermöglichen die Risiken zu verringern, den Zeitplan einzuhalten und weiteren Kostensteigerungen zu verhindern. Zusammenfassung der Einschätzungen Die aus Sicht der Experten größten Risiken sind im Folgenden zusammengestellt und bewertet: Technische Risiken: Die von der Gruppe untersuchten technischen Risiken scheinen prinzipiell lösbar, erfordern aber vermutlich mehr Zeit und Mittel als augenblicklich geplant. Nach Abschluss der in weiten Teilen bereits erfolgten Auftragsvergaben bergen die Qualitätskontrolle im Produktionsprozess sowie die Integration/Montage der Komponenten einschließlich des Änderungsmanagements noch mögliche Risiken und zählen deshalb zu den wichtigsten Aufgaben der Projektleitung.},
	author = {Bolt, Harald; Bernd, Löhr; Wolfgang, Meißner; Albrecht, Wagner},
	year = {2013},
	keywords = {/unread},
}

@article{dittmar_regional_2016,
	title = {Regional {Oil} {Extraction} and {Consumption}: {A} simple production model for the next 35 years {Part} {I}},
	issn = {2366-0112},
	doi = {10.1007/s41247-016-0007-7},
	abstract = {The growing conflicts in and about oil exporting regions and speculations about volatile oil prices during the last decade have renewed the public interest in predictions for the near future oil production and consumption. Unfortunately, studies from only 10 years ago, which tried to forecast the oil production during the next 20-30 years, failed to make accurate predictions for today's global oil production and consumption. Forecasts using economic growth scenarios, overestimated the actual oil production, while models which tried to estimate the maximum future oil production/year, using the official country oil reserve data, predicted a too low production. In this paper, a new approach to model the maximal future regional and thus global oil production (part I) and consumption (part II) during the next decades is proposed. Our analysis of the regional oil production data during past decades shows that, in contrast to periods when production was growing and growth rates varied greatly from one country to another, remarkable similarities are found during the plateau and decline periods of different countries. Following this model, the particular production phase of each major oil producing country and region is determined essentially only from the recent past oil production data. Using these data, the model is then used to predict the production from all major oil producing countries, regions and continents up to the year 2050. The limited regional and global potential to compensate this decline with unconventional oil and oil-equivalents is also presented.},
	author = {Dittmar, Michael},
	year = {2016},
	note = {arXiv: 1601.07716
ISBN: 2366-0112},
	keywords = {/unread},
}

