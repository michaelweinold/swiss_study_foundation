@article{murugathasan_moderate_2023,
	title = {Moderate exercise induces trained immunity in macrophages},
	issn = {0363-6143, 1522-1563},
	url = {https://journals.physiology.org/doi/10.1152/ajpcell.00130.2023},
	doi = {10.1152/ajpcell.00130.2023},
	abstract = {Despite its importance in protecting the host from infections and injury, excessive inflammation may lead to serious human diseases including autoimmune disorders, cardiovascular diseases, diabetes, and cancer. Exercise is a known immunomodulator; however, whether exercise causes long term changes in inflammatory responses and how these changes occur are lacking. Here, we show that chronic moderate intensity training of mice leads to persistent metabolic rewiring and changes to chromatin accessibility in bone marrow derived macrophages (BMDMs), which, in turn, tempers their inflammatory responses. We show that BMDMs from exercised mice exhibited a decrease in lipopolysaccharide (LPS) induced NF-kB activation and pro-inflammatory gene expression along with an increase in M2-like associated genes when compared to BMDMs from sedentary mice. This was associated with improved mitochondrial quality and increased reliance on oxidative phosphorylation accompanied with reduced mitochondrial ROS production. Mechanistically, ATAC-seq analysis showed changes in chromatin accessibility of genes associated with inflammatory and metabolic pathways. Overall, our data suggest that chronic moderate exercise can influence the inflammatory responses of macrophages by reprogramming their metabolic and epigenetic landscape.},
	language = {en},
	urldate = {2023-07-17},
	journal = {American Journal of Physiology-Cell Physiology},
	author = {Murugathasan, Mayoorey and Jafari, Ardavan and Amandeep, Amandeep and Hassan, Syed A and Chihata, Matthew and Abdul-Sater, Ali A.},
	month = jun,
	year = {2023},
	pages = {ajpcell.00130.2023},
}

@techreport{weinold_doctoral_2023,
	address = {Zurich, Switzerland},
	title = {Doctoral {Research} {Plan} {Michael} {Weinold}},
	copyright = {All rights reserved},
	abstract = {The air transport sector is expected to grow signiﬁcantly over the coming years, having already recovered to pre-pandemic levels ahead of original projections. Despite continuous improvements in fuel eﬃciency following the ﬂeet introduction of new families of jet aircraft and the associated engines, air travel remains the most carbon intensive mode of transportation. With aviation presently contributing around 2\% of global carbon emissions, multiple international organizations and national governments have set target to largely decarbonize the sector by 2050. However, the associated technical and economic challenges will require large-scale research and development expenditures. Already, research is being undertaken on large number of competing technologies, including battery-electric aircraft, hydrogen fuel-cells and various synthetic fuels and bio fuels. To ensure that the limited ﬁnancial resources available through public funding are allocated only to the most viable technologies, policy makers and research managers require reliable information about their environmental impact. In this context, life-cycle assessment has emerged as a major decision support tool. Following international standardization eﬀorts, the method is now widely used in academia and industry. However, present implementations are limited in both the precision and accuracy of their results.},
	language = {en},
	institution = {ETH Zurich},
	author = {Weinold, Michael},
	month = apr,
	year = {2023},
}

